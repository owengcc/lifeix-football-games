<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lifeix.football.games.module.player.mapper.TeamPlayerMapper">


	<insert id="insertOne" useGeneratedKeys="true" keyProperty="id" parameterType="com.lifeix.football.games.module.team.po.TeamPlayerPO">
	INSERT INTO `games`.`team_player` ( `teamid`, `playerid`, `position`, `createdate`) 
	VALUES (#{teamId}, #{playerId}, #{position}, #{createDate});
	</insert>

</mapper>