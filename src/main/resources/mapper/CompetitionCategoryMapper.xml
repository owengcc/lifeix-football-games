<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lifeix.football.games.module.competition.mapper.CompetitionCategoryMapper">

	<select id="findOne"  resultType="com.lifeix.football.games.model.CompetitionCategory">
		select * from competition_category where id=#{id}
	</select>
	<select id="findAll"  resultType="com.lifeix.football.games.model.CompetitionCategory">
		select * from competition_category 
	</select>
</mapper>