<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lifeix.football.games.module.jerseys.mapper.JerseysMapper">

	<select id="findOne" resultType="com.lifeix.football.games.model.Jerseys">
		select * from jerseys where
		teamid=#{id} order by startdate limit 1
	</select>

</mapper>